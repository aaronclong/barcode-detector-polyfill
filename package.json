{
  "name": "@aaronclong/barcode-detection-polyfill",
  "type": "module",
  "version": "0.1.0",
  "description": "Polyfill for the Barcode Detection API based on Dynamsoft Barcode Reader or ZXing.",
  "homepage": "https://github.com/aaronclong/barcode-detector-polyfill/#readme",
  "license": "MIT",
  "author": {
    "name": "Aaron Long"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/aaronclong/barcode-detector-polyfill/issues"
  },
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "dist/*.cjs",
    "dist/*.cjs.map",
    "dist/*.mjs",
    "dist/*.mjs.map",
    "dist/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronclong/barcode-detector-polyfill"
  },
  "contributors": [
    {
      "name": "Xu Lihang",
      "url": "https://github.com/xulihang/barcode-detector-polyfill"
    }
  ],
  "source": "src/barcode-detector.ts",
  "main": "./dist/barcode-detector.js",
  "exports": {
    "types": "./dist/barcode-detector.d.ts",
    "import": "./dist/barcode-detector.cjs",
    "require": "./dist/barcode-detector.cjs",
    "default": "./dist/barcode-detector.modern.js"
  },
  "module": "dist/foo.module.mjs",
  "unpkg": "dist/barcode-detector.umd.js",
  "amdName": "BarcodeDetectorPolyfill",
  "scripts": {
    "build": "microbundle build --globals @zxing/library=ZXingBrowser",
    "dev": "microbundle watch",
    "install:peers": "npx npm-install-peers -- -y"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "microbundle": "^0.15.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "barcode",
    "barcode-detector",
    "ean",
    "datamatrix",
    "pdf417",
    "qrcode"
  ],
  "dependencies": {},
  "peerDependencies": {
    "@zxing/library": "^0.20.0",
    "@zxing/browser": "^0.1.4"
  }
}
